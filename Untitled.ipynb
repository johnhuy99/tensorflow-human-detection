    "import cv2 as cv\n",
    "\n",
    "cap = cv.VideoCapture('/home/hp/Desktop/tensor-task8/people.mp4')\n",
    "while True:\n",
    "    _,frame = cap.read()\n",
    "    \n",
    "    cv.imshow('haha',frame)\n",
    "    \n",
    "    if cv.waitKey(25) & 0xff == ord('q'):\n",
    "        break\n",
    "cap.release()\n",
    "cv.destroyAllWindows()"
   ]
